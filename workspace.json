{
  "version": 2,
  "projects": {
    "effects": {
      "root": "libs/effects",
      "sourceRoot": "libs/effects/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/effects",
            "tsConfig": "libs/effects/tsconfig.lib.json",
            "project": "libs/effects/package.json",
            "entryFile": "libs/effects/src/index.ts",
            "external": [
              "react/jsx-runtime"
            ],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/effects/**/*.ts"
            ]
          },
          "outputs": [
            "{options.outputFile}"
          ]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/effects"
          ],
          "options": {
            "jestConfig": "libs/effects/jest.config.js",
            "passWithNoTests": true
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version"
        }
      }
    },
    "effects-ng": {
      "projectType": "library",
      "root": "libs/effects-ng",
      "sourceRoot": "libs/effects-ng/src",
      "prefix": "ngneat",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:package",
          "outputs": [
            "dist/libs/effects-ng"
          ],
          "options": {
            "project": "libs/effects-ng/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/effects-ng/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/effects-ng/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/effects-ng"
          ],
          "options": {
            "jestConfig": "libs/effects-ng/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/effects-ng/src/**/*.ts",
              "libs/effects-ng/src/**/*.html"
            ]
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version"
        }
      }
    },
    "hooks": {
      "root": "libs/hooks",
      "sourceRoot": "libs/hooks/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/hooks",
            "tsConfig": "libs/hooks/tsconfig.lib.json",
            "project": "libs/hooks/package.json",
            "entryFile": "libs/hooks/src/index.ts",
            "external": [
              "react/jsx-runtime"
            ],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/hooks/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/hooks"
          ],
          "options": {
            "jestConfig": "libs/hooks/jest.config.js",
            "passWithNoTests": true
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version"
        }
      }
    },
    "ng-web": {
      "projectType": "application",
      "root": "apps/ng-web",
      "sourceRoot": "apps/ng-web/src",
      "prefix": "effects",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/ng-web",
            "index": "apps/ng-web/src/index.html",
            "main": "apps/ng-web/src/main.ts",
            "polyfills": "apps/ng-web/src/polyfills.ts",
            "tsConfig": "apps/ng-web/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/ng-web/src/favicon.ico",
              "apps/ng-web/src/assets"
            ],
            "styles": [
              "apps/ng-web/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/ng-web/src/environments/environment.ts",
                  "with": "apps/ng-web/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ng-web:build:production"
            },
            "development": {
              "browserTarget": "ng-web:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-web:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ng-web/src/**/*.ts",
              "apps/ng-web/src/**/*.html"
            ]
          },
          "outputs": [
            "{options.outputFile}"
          ]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/ng-web"
          ],
          "options": {
            "jestConfig": "apps/ng-web/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "react-web": {
      "root": "apps/react-web",
      "sourceRoot": "apps/react-web/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/react-web",
            "index": "apps/react-web/src/index.html",
            "main": "apps/react-web/src/main.tsx",
            "polyfills": "apps/react-web/src/polyfills.ts",
            "tsConfig": "apps/react-web/tsconfig.app.json",
            "assets": [
              "apps/react-web/src/favicon.ico",
              "apps/react-web/src/assets"
            ],
            "styles": [
              "apps/react-web/src/styles.scss"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/react-web/src/environments/environment.ts",
                  "with": "apps/react-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "react-web:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "react-web:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/react-web/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/react-web"
          ],
          "options": {
            "jestConfig": "apps/react-web/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/web"
  },
  "generators": {
    "@nrwl/web:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/web:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "react-web"
}